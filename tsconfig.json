{
    "compilerOptions": {
      "target": "es2016",
      "module": "CommonJS",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "moduleResolution": "node",
      "experimentalDecorators": true
    },
    "include": [
      "app/**/*.ts"
    ],
    "exclude": [
      "node_modules"
    ]
  }